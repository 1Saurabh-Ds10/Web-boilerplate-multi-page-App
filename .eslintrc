{
  "extends": "airbnb",
  "rules": {
    "linebreak-style": ["error", "windows"],
    "comma-dangle": 0,
    "object-curly-spacing": 0,
    "arrow-body-style": 0,
    "react/jsx-filename-extension": [1, { "extensions": [".js", ".jsx"] }],
    "react/prop-types": 0,
    "jsx-a11y/no-static-element-interactions": 0,
    "react/jsx-tag-spacing": 0,
    "react/jsx-closing-bracket-location": 0,
    "no-else-return": 0,
    "arrow-parens": 0,
    "jsx-a11y/href-no-hash": 0,
    "no-alert":0,
    "consistent-return":0,
    "no-underscore-dangle": 0,
    "no-console":0
  },
  "parser": "babel-eslint",
  "parserOptions": {
        "ecmaVersion": 6,
        "sourceType": "module",
        "ecmaFeatures": {
            "jsx": true
        }
    },
      "env": {
        "browser": true,
        "node": true
    }
 
  
}